{
    "manifest_version": 3,
    "name": "Gerenciador de Senhas",
    "version": "1.0",
    "description": "Extens√£o para gerenciamento de senhas e monitoramento de acessos.",
    "host_permissions": ["http://127.0.0.1:5000/*"],
    "permissions": ["storage", "activeTab", "tabs", "cookies"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "image.ico"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}
  